<div class="container">
 <h1>DBC Stack Overflow</h1>
 <p>A language-independent collaboratively edited question and answer site for programmers</p>

 <a href='/sessions/new'>Login</a> |
 <a href='/users/new'>Register</a>


 <% @questions.each do |question| %>

 	<article id="<%= question.id %>">
 	<form method="post" action='/question/<%= question.id %>/vote' class="inline">
    	<button type="submit" name="submit_param" value="submit_value" class="upvote-button"></button>
     </form>
 	<h2><a href='/questions/<%= question.id %>'><%= question.title %></a></h2>
 	<p>
      <a class="delete" href='/question/<%= question.id %>'></a>
    </p>
   	</article>
 <% end %>

<form id="posts" method="post" action="/questions">
 <input type="text" name="title" placeholder="Question">
 <input type="submit" value="submit new post">
</form>